<script>


    $(document).ready(function () {
        debugger;

        $('.loader-wrapper').show();
        LoadSortTableFill();
        var sorted = 'order by Title asc, Status asc';
        $("#SortValuee").val(sorted);
        $('#divSuccess').hide();
        //$.get('/ACompanyManagement/IndexreUserPaging', { PageSize: 0, PageIndex: 1, Search: '', Sort: sorted }, function (response) {
        //    $("#RawDatas table tbody tr:eq(0)").hide();
        //    $(".loader-wrapper").hide();
        //});


    });

    $("#btnsearch").click(function () {


        $("#loading1").show();
        LoadSortTableFill();
    });

    $("#btnsearchVendor").click(function () {
        LoadSortTableFillWithSearch();
    });

    function LoadSortTableFill() {
        debugger;
        $('#PropertyMessage').hide();
        $('#PropertyMessage').find('#propertyspan').html('');
        $.ajax({
            url: '@Url.Action("IndexrePropertyPaging", "ACompanyManagement")',
            cache: false,
            data: {
                PageSize: $('#PageSize').val(),
                PageIndex: $('#HdnPageIndex').val(),
                Search: $('#txtsearch').val(),
                Sort: $("#HdnSortValue").val()
            },
            success: function (response) {
                debugger;
                $("#RawDatas").show();
                if (parseInt(response.length) > 0) {

                    var curRecords = 0;
                    var TotalRecs = 0;
                    debugger;
                    curRecords = parseInt(($('#HdnPageIndex').val()) * parseInt($('#PageSize').val()));
                    curRecords = curRecords - ($('#PageSize').val() - parseInt(response.length));
                    TotalRecs = parseInt(response[0].TotalRecords);
                    $('.spnOfRecs').text(curRecords);
                    $('.propertyTotalRecs').text(TotalRecs);

                    if (response[0].TotalRecords !== "0") {
                        $("#btnProperty").html("Property (" + response[0].TotalRecords + ")");

                    }

                } else {
                    $("#RawDatas").hide();
                }
                $("#RawDatas table tbody tr:not(:first)").remove();
                $("#RawDatas table tbody tr:eq(0)").show();
                for (var i = 0; i < response.length; i++) {
                    var table = $("#RawDatas table tbody tr:eq(0)").clone(true);
                    $(".title", table).html(response[i].Title);
                    $(".address", table).html(response[i].Address);
                    $(".numberofunits", table).html(formatNumber(response[i].NumberOfUnits));
                    $("#viewdata", table).attr('href', '/ACompanyManagement/APropertiesView?PropertyKey=' + response[i].PropertyKey);

                    $("#RawDatas table").append(table);
                }
                $("#RawDatas table tbody tr:eq(0)").hide();

                if ($("#RawDatas table tbody tr:not(:first)").length == 0) {
                    $('#PropertyMessage').show();
                    $('#PropertyMessage').find('#propertyspan').html('Property does not exists for this company.');
                }

                $("#loading1").hide();
                //updatePaging(response[0].TotalRecords);

            }
        });
    }


</script>


<script>

    $(document).ready(function () {
        //$("#statuscss").css("color", "green");
        LoadSortTableFilll();
        $('#divSuccess').hide();

        //$('#divSuccess').show();
    });




    function hide() {
        debugger;
        $("#hidemsg").hide();
    }
    function LoadSortTableFilll() {
        debugger;
        $('#UserMessage').hide();
        $('#UserMessage').find('#userspan').html('');
        $.ajax({
            url: '@Url.Action("IndexreCUserPaging", "ACompanyManagement")',
            cache: false,
            data:
            {
                CompanyKey: $('#CompanyKey').val()
            },
            success: function (response) {

                if (parseInt(response.length) > 0) {

                    var curRecords = 0;
                    var TotalRecs = 0;
                    debugger;
                    curRecords = parseInt(($('#HdnPageIndex').val()) * parseInt($('#PageSize').val()));
                    curRecords = curRecords - ($('#PageSize').val() - parseInt(response.length));
                    TotalRecs = parseInt(response[0].TotalRecords);
                    $('.spnOfRecs').text(curRecords);
                    $('.usersTotalRecs').text(TotalRecs);
                    if (response[0].TotalRecords !== "0") {
                        $("#btnUser").html("Users (" + response[0].TotalRecords + ")");

                    }
                    if (curRecords === 0 || curRecords === TotalRecs) {
                        $("#ddlload").hide();
                    }
                    else {
                        $("#ddlload").show();
                    }
                } else {
                    $("#ddlload").hide();
                }
                $("#RawDatasss table tbody tr:not(:first)").remove();
                $("#RawDatasss table tbody tr:eq(0)").show();
                for (var i = 0; i < response.length; i++) {
                    var table = $("#RawDatasss table tbody tr:eq(0)").clone(true);
                    $(".nameee", table).html(response[i].FirstName);

                    $(".email", table).html(response[i].Email);
                    if (response[i].PrimaryContact == "true" || response[i].PrimaryContact == true || response[i].PrimaryContact == "True") {
                        //alert(" Y  " + i + "primaryContact " + response[i].PrimaryContact);
                        $("#primarycontact", table).css("badge badge-primary", "Yes");
                        $("#primsss", table).html('Yes');
                    }
                    else {
                        $("#primarycontact", table).css("badge badge-white", "No");
                        $("#primsss", table).html('No');
                    }
                    debugger;
                    $("#viewdatass", table).attr('href', '/ACompanyManagement/AStaffDirectoryView?ResourceKey=' + response[i].ResourceKey);

                    $("#RawDatasss table").append(table);

                }
                $("#RawDatasss table tbody tr:eq(0)").hide();

                if ($("#RawDatasss table tbody tr:not(:first)").length == 0) {
                    $('#UesrMessage').show();
                    $('#UesrMessage').find('#userspan').html('User does not exists for this company');
                }

                $("#loading1").hide();
            }
        });
    }



</script>
