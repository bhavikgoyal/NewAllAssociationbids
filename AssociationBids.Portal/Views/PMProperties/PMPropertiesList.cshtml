        $('#HdnPageIndex').val();
        pageSize = parseInt(pageSize * CountVal);

        $("#loading1").show();
        $.ajax({
            type: "Get",
            url: "/PMProperties/IndexreUserPaging",
            data:
            {
                PageSize: pageSize,
                PageIndex: $('#HdnPageIndex').val(),
                Search: $('#txtsearch').val(),
                Sort: $("#HdnSortValue").val()
            },
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: OnSuccess,
            failure: function (response)
            {
            },
            error: function (response)
            {
            }
        });
    }
    function SortingFun(val) {

        $('#HdnPageIndex').val('1');
        var Sort = $('#SortValue').val();
        //debugger;
        var ascdesc  = 'fa-sort-asc';
        var ascdesc1 = 'fa-sort-desc';
        var ascdesc3 = 'fa-sort-asc';
        var ascdesc4 = 'fa-sort-desc';

        $('.loader-wrapper').show();
        var Sort = '';
        $('#HdnPageIndex').val('1');
        $('#Titlei').removeClass(ascdesc4);
        $('#Titlei').addClass(ascdesc3);

        $('#Addressi').removeClass(ascdesc4);
        $('#Addressi').addClass(ascdesc3);

        $('#NumberOfUnitsi').removeClass(ascdesc4);
        $('#NumberOfUnitsi').addClass(ascdesc3);

        //debugger;
        //-----------------------------------------------------------
        if (val == 'Title') {
            if ($('#Titlehdn').val() == 'asc') {
                Sort = 'order by u.Title desc';
                $('#Titlehdn').val('desc');
                $('#Titlei').removeClass(ascdesc);
                $('#Titlei').addClass(ascdesc1);
            }
            else {
                Sort = 'order by u.Title asc';
                $('#Titlehdn').val('asc');
                $('#Titlei').removeClass(ascdesc1);
                $('#Titlei').addClass(ascdesc);
            }
        }
        //-------------------------------------------------
        if (val == 'Address') {
            if ($('#Addresshdn').val() == 'asc') {
                Sort = 'order by u.Address desc';
                $('#Addresshdn').val('desc');
                $('#Addressi').removeClass(ascdesc);
                $('#Addressi').addClass(ascdesc1);
            }
            else {
                Sort = 'order by u.Address asc';
                $('#Addresshdn').val('asc');
                $('#Addressi').removeClass(ascdesc1);
                $('#Addressi').addClass(ascdesc);
            }
        }
        //-------------------------------------------------------
        if (val == 'NumberOfUnits') {
            if ($('#NumberOfUnitshdn').val() == 'asc') {
                Sort = 'order by u.NumberOfUnits desc';
                $('#NumberOfUnitshdn').val('desc');
                $('#NumberOfUnitsi').removeClass(ascdesc);
                $('#NumberOfUnitsi').addClass(ascdesc1);
            }
            else {
                Sort = 'order by u.NumberOfUnits asc';
                $('#NumberOfUnitshdn').val('asc');
                $('#NumberOfUnitsi').removeClass(ascdesc1);
                $('#NumberOfUnitsi').addClass(ascdesc);
            }
        }


        $("#HdnSortValue").val(Sort);
        $('.loader-wrapper').show();
        LoadSortTableFill();
    }

    function OnSuccess(response)
    {
        var pageSize = '@System.Configuration.ConfigurationManager.AppSettings["PageSize"].ToString()';
        //debugger;

        $('.spnOfRecs').text(0);
        $('.spnTotalRecs').text(0);
        $('.spnToRecs').text(0);

        if (parseInt(response.length) > 0) {
            var curRecords = 0;
            var TotalRecs = 0;
            var spnToRecs = 0;
            //debugger;
            curRecords = parseInt(($('#HdnPageIndex').val()) * parseInt(pageSize));
            curRecords = curRecords - (pageSize - parseInt(response.length));
            TotalRecs = parseInt(response[0].TotalRecords);
            $('.spnOfRecs').text(curRecords);
            $('.spnTotalRecs').text(TotalRecs);


        } else {

        }
        $("#RawDatas table tbody tr:not(:first)").remove();
        $("#RawDatas table tbody tr:eq(0)").show();
        for (var i = 0; i < response.length; i++) {
            var table = $("#RawDatas table tbody tr:eq(0)").clone(true);
            $(".title", table).html(response[i].Title);
            $(".address", table).html(response[i].Address);
            $(".numberofunits", table).html(formatNumber(response[i].NumberOfUnits));

            $("#viewdata", table).attr('href', '/PMProperties/PropertiesView?PropertyKey=' + response[i].PropertyKey);

            $("#RawDatas table").append(table);
            spnToRecs++;

        }
        $("#RawDatas table tbody tr:eq(0)").hide();

        spnToRecs = (curRecords - spnToRecs) + 1;
        if (spnToRecs > 0) {
            $('.spnToRecs').text(spnToRecs);
        }
        else {
            $('.spnToRecs').text(0);
        }
        if (curRecords.text == "NaN") {

            $('.errormessage').html('Data Is Not Found.');
            $('#Error').show();
            $("#RawDatas table thead tr:eq(0)").hide();
            $("#RawDatas table tbody tr:eq(0)").hide();
        }
        else {
            updatePaging(response[0].TotalRecords);
            //$("#loading1").hide();
        }
    }
    function formatNumber(num) {

        return new Number(num).toLocaleString('en-US', {
            maximumFractionDigits: 2
        });
    }
</script>

