    function GetNotificationCount() {
        $.ajax({
            url: '/ABNotification/GetNotificationCount',
            success: function (response) {
                if (response.length > 0) {
                    debugger;
                    var refundcount = 0;
                    var regUsercount = 0;
                    var tabvendcount = 0;
                    for (var i = 0; i < response.length; i++) {
                        if (response[i].ModuleKey == 200) {
                            refundcount += response[i].NewNotificationCount;
                        }
                        else if (response[i].ModuleKey == 305) {
                            regUsercount += response[i].NewNotificationCount;
                        }
                    }
                    tabvendcount = refundcount + regUsercount;
                    if (refundcount > 0) {
                        $("#vendorRefundI").html("Vendors Refunds &nbsp;<i class='badge badge-warning'>" + refundcount + "</i>");
                    }
                    if (regUsercount > 0) {
                        $("#Unap1").html("Vendor Approvals &nbsp;<i class='badge badge-warning'>" + regUsercount + "</i>");
                    }
                    if (tabvendcount > 0) {
                        $(".vendspan").html("Vendors &nbsp;<i class='badge badge-warning'>" + tabvendcount + "</i>");
                    }
                }
            }
        });
    }

    function LoadCountNewMsg() {
        $.ajax({
            url: '../VenderBidrequest/MessageNewCount',
            cache: false,
            data: {
                ObjectKey: 0,
                ResourceKey: 0,
                ModuleKeyName: $("#HdnModuleKeyName").val(),
            },
            success: function (response) {
                response = JSON.parse(response);
                var b = 0;
                var w = 0;
                if (parseInt(response.length) > 0) {
                    if (response[0].TotalManagerBidRequest !== "0") {
                        //b = response[0].MessageStatusNewPM;
                        b = response[0].TotalManagerBidRequest;
                        $("#BidRequestsMenu").html("Bid Requests (" + b + ")");
                    }
                    if (response[0].TotalManagerWorkOrder !== "0") {
                        //w = response[0].MessageStatusNewPMWO;
                        w = response[0].TotalManagerWorkOrder;
                        $("#WorkorderMenu").html("Work Orders (" + w + ")");
                    }
                    else {
                        $("#BidRequestsMenu").html("Bid Requests");
                        $("#WorkorderMenu").html("Work Orders");
                    }
                    //  GetNotificationCount(b, w);
                }
                $(".loader-wrapper").hide();
            },
            error: function () {
                //GetNotificationCount();
            }
        });
    }
</script>

