        function LoadCountNewMsg() {
            $.ajax({
                url: '../VenderBidrequest/MessageNewCount',
                cache: false,
                data: {
                    ObjectKey: 0,
                    ResourceKey: 0,
                    ModuleKeyName: $("#HdnModuleKeyName").val(),
                },
                success: function (response)
                {
                    response = JSON.parse(response);
                    var b = 0;
                    var w = 0;
                    if (parseInt(response.length) > 0)
                    {
                        if (response[0].TotalManagerBidRequest !== "0")
                        {
                            //b = response[0].MessageStatusNewPM;
                            b = response[0].TotalManagerBidRequest;
                            $("#BidRequestsMenu").html("Bid Requests (" + b + ")");
                        }
                        if (response[0].TotalManagerWorkOrder !== "0")
                        {
                            //w = response[0].MessageStatusNewPMWO;
                            w = response[0].TotalManagerWorkOrder;
                            $("#WorkorderMenu").html("Work Orders (" + w + ")");
                        }
                        else
                        {
                            $("#BidRequestsMenu").html("Bid Requests");
                            $("#WorkorderMenu").html("Work Orders");
                        }
                      //  GetNotificationCount(b, w);
                    }
                    $(".loader-wrapper").hide();
                },
                error: function () {
                    //GetNotificationCount();
                }
            });
        }
