    function formatPhoneNumber(phoneNumberString) {
        debugger;
        var x = phoneNumberString;
        if (phoneNumberString.toString().length == 10) {
            //var x = phoneNumberString.replace(/[^\d]+/g, '').replace(/(\d{1})(\d{3})(\d{3})(\d{4})/, '+$1 ($2) $3-$4');
            x = phoneNumberString.replace(/[^\d]+/g, '').replace(/(\d{3})(\d{3})(\d{4})/, '($1) $2-$3');
        }
        return x;
    }
    function formatNumber(num, isCurrency = false) {
        debugger;
        if (isCurrency) {
            return new Number(num).toLocaleString('en-US', {
                maximumFractionDigits: 2,
                minimumFractionDigits: 2
            });
        }
        return new Number(num).toLocaleString('en-US', {
            maximumFractionDigits: 2
        });
    }

    function SetActiveMenu(menu, isMainMenu = true) {
        debugger;
        var p = 0
        var c = menu;
        if (isMainMenu == false) {
            while (c[0].className != "slide") {
                c.addClass("active");
                c = c.parent();
                if (c[0].className == 'slide') {
                    c.addClass("is-expanded");
                    c.children().closest('a').addClass("active");
                }
                p++;
                if (p > 5)
                    break;

            }
        } else {
            c.addClass("active");
        }
    }
    function GetNotificationCount(bmsg = 0, wmsg = 0) {
        debugger;
        $.ajax({
            url: '/ABNotification/GetNotificationCount',
            success: function (response) {
                if (response.length > 0) {
                    debugger;
                    var bidcount = 0;
                    var workcount = 0;
                    var pcount = 0;
                    var ccount = 0;
                    var mcount = 0;
                    var acCount = 0;
                    for (var i = 0; i < response.length; i++) {
                        if (response[i].ModuleKey == 100) {
                            bidcount += response[i].NewNotificationCount;
                        }
                        else if (response[i].ModuleKey == 106) {
                            workcount += response[i].NewNotificationCount;
                        }
                        else if (response[i].ModuleKey == 300) {
                            mcount += response[i].NewNotificationCount;
                        }
                        else if (response[i].ModuleKey == 301) {
                            ccount   += response[i].NewNotificationCount;
                        }
                        else if (response[i].ModuleKey == 302) {
                            pcount += response[i].NewNotificationCount;
                        }
                    }
                    acCount = pcount + mcount + ccount;
                    if (bidcount > 0) {
                        var b = 'Bid Requests';
                        if (bmsg > 0)
                            b += ' (' + bmsg + ')';
                        $("#BidRequestsMenu").html(b + " (" + bidcount + ")");
                    }
                    if (workcount > 0) {
                        var w = 'Work Order';
                        if (wmsg > 0)
                            w += ' (' + wmsg + ')';
                        $("#WorkOrderMenu").html(w + " (" + workcount + ")");
