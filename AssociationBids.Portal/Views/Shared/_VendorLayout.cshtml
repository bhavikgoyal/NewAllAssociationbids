        function LoadCountNewMsg() {
            $.ajax({
                url: '../VenderBidrequest/MessageNewCount',
                cache: false,
                data: {
                    ObjectKey: 0,
                    ResourceKey: 0,
                    ModuleKeyName: $("#HdnModuleKeyName").val(),
                },
                success: function (response) {
                    response = JSON.parse(response);
                    var b = 0;
                    var w = 0;
                    if (parseInt(response.length) > 0) {
                        if (response[0].TotalOpenBidRequest !== "0") {
                          //  b = response[0].NewMessagecnt;
                            b = response[0].TotalOpenBidRequest;
                            $("#BidRequestsMenu").html("Bid Requests (" + b + ")");

                        } else { $("#BidRequestsMenu").html("Bid Requests"); }
                        if (response[0].TotalWorkOrder !== "0") {
                          //  w = response[0].NewMessagecntWO;
                            w = response[0].TotalWorkOrder;                            
                            $("#WorkOrderMenu").html("Work Order (" + w + ")");
                        } else { $("#WorkOrderMenu").html("Work Order"); }
                    }
                    //GetNotificationCount(b,w);
                    $(".loader-wrapper").hide();
                },
                error: function () {
                    //GetNotificationCount();
                }
            });
        }
    </script>
