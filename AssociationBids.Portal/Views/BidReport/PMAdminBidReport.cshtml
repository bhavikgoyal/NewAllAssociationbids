                        $("#red", table).hide();
                        $("#yellow", table).hide();
                        $("#green", table).hide();
                    }


                        $(".PropertyName", table).html(response[i].Property);
                        //if (response[i].NewMessageCount > 0 || response[i].priority == '1')
                        //{
