                $("#bidRequestkey").val(CblVendor);
                if (response.length > 0) {
                    $("#ispriorityrecord").val(response[0].ispriorityrecord);
                }
                spnToRecs = (curRecords - spnToRecs) + 1;
                if (spnToRecs > 0) {
