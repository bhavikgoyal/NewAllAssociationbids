    function LoadMessagesList_model(msgstatus) {
        debugger;
        $.ajax({

            url: '../VendorManager/SendInsertMessageList',
            cache: false,
            data: {
                ObjectKey: $("#HdnObjectKey").val(),
                ResourceKey: $("#HdnLoginResourceKey").val(),
                ModuleKeyName: $("#HdnModuleKeyName").val()
                , UpdatemsgStatus: msgstatus
            },
            success: function (response) {
                debugger;
                response = JSON.parse(response);
                if (parseInt(response.length) > 0) {
                    //$("#MsgTab_model").html("Message (" + response.length + ")");
                    $("#MessageRawDatas_model div.table123:not(:first)").remove();
                    $("#MessageRawDatas_model div.table123:eq(0)").show();
                    for (var i = 0; i < response.length; i++) {

                        if (response[i].NewMessagecnt !== "0") {
                            $("#BidRequestsMenu_model").html("Bid Requests (" + response[i].NewMessagecnt + ")");
                        } else { $("#BidRequestsMenu_model").html("Bid Requests"); }
                        var table = $("#MessageRawDatas_model div.table123:eq(0)").clone(true);

                        if (response[i].RightLeft === 'Right') {
                            $("#DivRightSide_model", table).show();
                            $("#DivLeftSide_model", table).hide();
                            $("#DivLeftSide_model", table).attr("style", "display:none !important;");

                            $(".namefirstletter_model", table).html(response[i].FirstName);
                            $("#SendMesg_model", table).html(response[i].Body);
                            $("#SendTime_model", table).html(response[i].SendDate + ' ' + response[i].SendTime);
                        } else {
                            $("#DivLeftSide_model", table).show();
                            $("#DivRightSide_model", table).hide();
                            $("#DivRightSide_model", table).attr("style", "display:none !important;");

                            $(".namefirstletter_model", table).html(response[i].FirstName);
                            $("#ReceiveMesg_model", table).html(response[i].Body);
                            $("#ReceiveTime_model", table).html(response[i].SendDate + ' ' + response[i].SendTime);
                        }
                        //$(table).show();
                        $("#MessageRawDatas_model").append(table);
                    }
                    // $("#MessageRawDatas table tbody tr:eq(0)").hide();
                    $("#MessageRawDatas_model div.table123:eq(0)").hide();
                    //LoadCountNewMsg();
                }
                $(".loader-wrapper").hide();
            }
        });
    }

    function BindVendor()
    {
        debugger;
        //alert("bindserviceVendorKey " + a);
        var formData = new FormData();
        var a = $("#BidRequest_BidRequestKey").val();
        var b = 106;
        formData.append("BidRequestKey", a);
        formData.append("ModuleKey", b);
        //alert("Bind Service After FormData");
       debugger;

        $.ajax({
            type: 'POST',
            datatype: 'json',
            url: '@Url.Action("SearchVendorByBidRequest", "VendorManager")',
            data: formData,
            contentType: false,
            processData: false,
            success: function (response) {
                //alert("on Service Success");
                $("#ServiceVendorView table tbody tr:not(:first)").remove();
                $("#ServiceVendorView table tbody tr:eq(0)").show();
                for (var i = 0; i < response.length; i++) {
                    var cln = $("#ServiceVendorView table tbody tr:eq(0)").clone(true);
                    //alert("title = " + response[i].ServiceTitle);
                    $(".vVendorName", cln).html(response[i].ContactPerson);
                    $(".vResponseDueDate", cln).html(response[i].ResponseDueDates);
                    $(".vBidAmount", cln).html("$"+formatNumber(response[i].BidAmount,true));
                    $(".vStatus", cln).html(response[i].BidReqStatus);
                    $(".bidvendorkey", cln).val(response[i].BidVendorKey);

                    $("#viewreview", cln).attr('onclick', 'return openpopup("' + response[i].BidVendorKey + '","' + response[i].BidAmount + '","' + response[i].BidReqStatus + '","' + response[i].ResponseDueDates +'")');
                    $("#ServiceVendorView table").append(cln);
                }
                $("#ServiceVendorView table tbody tr:eq(0)").hide();
            }
        });
    }
